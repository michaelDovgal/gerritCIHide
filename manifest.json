{
  "manifest_version": 2,

  "name": "Hide annoying results",
  "description": "Hide annoying tests results",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
        "contextMenus", 
        "tabs",
        "http://*/*",
        "https://*/*"
  ],
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["jquery-3.1.1.min.js","content.js"],
        "run_at": "document_end"
      }
    ]
}